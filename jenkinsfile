pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                checkout scmGit(
                    branches: [[name: '*/master']],
                    userRemoteConfigs: [[credentialsId: 'serch',
                        url: 'https://github.com/serugio/goRest']])
            }
        }
        stage('Dependencies') {
           steps {
               bat 'npm install'
           }
       }
        stage('Test') {
            steps {
                bat 'npm run postman-test'
            }
        }
    }
    // post {
    //     always {
    //         archiveArtifacts artifacts: 'mochawesome-report/**/*.*', fingerprint: true
    //     }
    // }
}
